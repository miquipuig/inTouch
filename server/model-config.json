{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false
  },
 
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "user": {
    "dataSource": "mongodb",
    "public": true
  },
  "listing": {
    "dataSource": "mongodb",
    "public": true
  },
  "review": {
    "dataSource": "mongodb",
    "public": true
  },
  "announcement": {
    "dataSource": "mongodb",
    "public": true
  },
  "reservation": {
    "dataSource": "mongodb",
    "public": true
  },
  "container": {
    "dataSource": "filesystemContainer",
    "public": true
  },
  "chat": {
    "dataSource": "mongodb",
    "public": true
  },
  "chatMessage": {
    "dataSource": "mongodb",
    "public": true
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": true,
    "acls": [  {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }]
  }


}
