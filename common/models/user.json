{
  "name": "user",
  "base": "User",
  "strict": true,
  "idInjection": true,
  "properties": {
    "nombre": {
      "type": "string"
    },
    "apellido": {
      "type": "string"
    },
    "telefono": {
      "type": "string"
    },
    "intereses": {
      "type": [
        "string"
      ]
    },
    "direccion": {
      "type": "string"
    },
    "identificacion": {
      "type": "string"
    },
    "trainer": {
      "type": "boolean"
    },
    "favoriteListings": {
      "type": [
        "string"
      ]
    }
  },
  "validations": [],
  "relations": {
    "reviews": {
      "type": "hasMany",
      "model": "review",
      "foreignKey": "reviewPublisherId"
    },
    "listings": {
      "type": "hasMany",
      "model": "listing",
      "foreignKey": "trainerPublisherId"
    },
    "announcements": {
      "type": "hasMany",
      "model": "announcement",
      "foreignKey": "trainerPublisherId"
    },
     "reservations": {
      "type": "hasMany",
      "model": "reservation",
      "foreignKey": "userReservationId"
    }
    
    
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "addFavoriteListing"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "removeFavoriteListing"
    }
  ],
  "methods": {}
}
